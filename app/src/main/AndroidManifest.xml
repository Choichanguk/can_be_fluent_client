<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.canbefluent">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".practice_autoGPS"></activity>
        <activity android:name=".practice.gps_practice" />
        <activity android:name=".user_profile_activity" />
        <activity android:name=".practice.practice_img_upload" />
        <activity
            android:name=".lang_level_popup"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".practice.img_upload_exam" />
        <activity
            android:name=".countryListActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity
            android:name=".datePickerActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".Register_setLanguage" />
        <activity android:name=".Register_setProfile" />
        <activity android:name=".Register_setId" />
        <activity android:name=".Login">
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>